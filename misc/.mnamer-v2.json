{
    "api_key_omdb": null,
    "api_key_tmdb": null,
    "api_key_tvdb": null,
    "api_key_tvmaze": null,
    "batch": true,
    "episode_api": "tvdb",
    "episode_directory": null,
    "episode_format": "{series} S{season:01} E{episode:01} - {title}.{extension}",
    "hits": 5,
    "ignore": [
        ".*sample.*",
        "^RARBG.*"
    ],
    "language": null,
    "lower": false,
    "mask": [
        ".avi",
        ".m4v",
        ".mp4",
        ".mkv",
        ".ts",
        ".wmv",
        ".srt",
        ".idx",
        ".sub"
    ],
    "movie_api": "tmdb",
    "movie_directory": null,
    "movie_format": "{name} ({year}).{extension}",
    "no_guess": false,
    "no_overwrite": false,
    "no_style": false,
    "recurse": false,
    "replace_after": {
        "&": "and",
        ";": ",",
        "@": "at"
    },
    "replace_before": {},
    "scene": false,
    "verbose": false
}
